# Environment variables for Policast deployment
# Copy this file to .env and fill in your values

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# ERC20 token address to use for betting
BETTING_TOKEN=0x0000000000000000000000000000000000000000

# Optional: Address to grant resolver role (leave empty if not needed)
RESOLVER_ADDRESS=

# Optional: Etherscan API key for contract verification
ETHERSCAN_API_KEY=